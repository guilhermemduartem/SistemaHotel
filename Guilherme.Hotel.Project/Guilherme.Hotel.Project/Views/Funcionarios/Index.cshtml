@model IEnumerable<Guilherme.Hotel.Project.Models.Funcionarios>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Salario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Turno)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cargo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumConvMedico)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumConOndoto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumContaCorrente)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NumAgencia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Banco)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Salario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Turno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cargo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumConvMedico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumConOndoto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumContaCorrente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumAgencia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Banco)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
